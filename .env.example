# Canvas Environment Variables
# Copy this file to .env.local for development

# Supabase (Required)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# AI Services (Required)
VITE_OPENROUTER_API_KEY=sk-or-v1-your-key
VITE_PERPLEXITY_API_KEY=pplx-your-key
VITE_CLAUDE_API_KEY=sk-ant-your-key # Optional, for direct Claude access

# Search & Scraping (Required)
VITE_BRAVE_SEARCH_API_KEY=your-brave-key
VITE_FIRECRAWL_API_KEY=fc-your-key
VITE_SCRAPER_SERVICE_URL=http://localhost:3001 # Production: https://your-server.com:3001

# Communication (Required for outreach features)
VITE_SENDGRID_API_KEY=SG.your-key
VITE_TWILIO_ACCOUNT_SID=ACyour-sid
VITE_TWILIO_AUTH_TOKEN=your-auth-token
VITE_TWILIO_PHONE_NUMBER=+1234567890

# Stripe (Required for payments)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-key # Production: pk_live_your-key

# NPI Registry (Optional - uses public API)
VITE_NPI_API_URL=https://npiregistry.cms.hhs.gov/api

# Google Analytics (Optional)
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Feature Flags (Optional)
VITE_ENABLE_TELEMETRY=true
VITE_ENABLE_PREMIUM_FEATURES=false

# Legacy React App Variables (being phased out)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here
REACT_APP_API_URL=http://localhost:3001
REACT_APP_USE_STREAMLINED=true
REACT_APP_USE_SUPER_INTELLIGENCE=false
REACT_APP_USE_LOCAL_CLAUDE=false
REACT_APP_LOCAL_CLAUDE_ENDPOINT=http://localhost:8080/claude
REACT_APP_LOCAL_CLAUDE_KEY=your-local-key